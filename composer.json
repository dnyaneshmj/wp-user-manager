{
  "require": {
    "a5hleyrich/wp-background-processing": "^1.0",
    "alessandrotesoro/wp-notices": "^1.0",
    "brain/cortex": "~1.0.0",
    "gamajo/template-loader": "^1.3",
    "htmlburger/carbon-fields": "^2.1",
    "typisttech/imposter-plugin": "^0.6.0@dev",
    "wearerequired/wp-requirements-check": "^1.0",
    "wp-user-manager/wp-optionskit": "^1.0",
    "wp-user-manager/wpum-blocks": "^1.0",
    "wpbp/widgets-helper": "dev-master"
  },
  "minimum-stability": "dev",
  "scripts": {
    "post-install-cmd": [
      "sed -i -e 's/new \\\\Carbon_Fields/new \\\\WPUM\\\\WPUM\\\\Carbon_Fields/g' ./vendor/htmlburger/carbon-fields/core/Carbon_Fields.php",
      "sed -i -e 's/new \\\\Carbon_Fields/new \\\\WPUM\\\\WPUM\\\\Carbon_Fields/g' ./vendor/htmlburger/carbon-fields/core/Provider/Container_Condition_Provider.php"
    ],
    "post-autoload-dump": [
      "sed -i -e 's/new \\\\Carbon_Fields/new \\\\WPUM\\\\WPUM\\\\Carbon_Fields/g' ./vendor/htmlburger/carbon-fields/core/Carbon_Fields.php",
      "sed -i -e 's/new \\\\Carbon_Fields/new \\\\WPUM\\\\WPUM\\\\Carbon_Fields/g' ./vendor/htmlburger/carbon-fields/core/Provider/Container_Condition_Provider.php",
      "sed -i -e 's/namespace Carbon_Fields/namespace WPUM\\\\WPUM\\\\Carbon_Fields/g' ./vendor/htmlburger/carbon-fields/config.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "imposter": {
      "namespace": "WPUM\\WPUM",
      "excludes": ["wp-user-manager/wpum-blocks"]
    }
  }
}
